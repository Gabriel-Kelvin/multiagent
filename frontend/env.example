# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# Backend API URL
# For local development:
# VITE_API_BASE_URL=http://localhost:8017
# For production/VM deployment (replace with your VM IP):
VITE_API_BASE_URL=http://18.142.146.189:8017

# Supabase Authentication
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application URL (for redirects and callbacks)
# For local development:
# VITE_APP_URL=http://localhost:8018
# For production/VM deployment (replace with your VM IP):
VITE_APP_URL=http://18.142.146.189:8018

# Public Site Name (optional)
VITE_PUBLIC_SITE_NAME=MultiAgent
